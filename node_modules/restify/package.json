{
  "name": "restify",
  "description": "REST framework specifically meant for web service APIs",
  "version": "0.3.15",
  "repository": {
    "type": "git",
    "url": "git://github.com/mcavage/node-restify.git"
  },
  "author": "Mark Cavage <mcavage@gmail.com> (http://www.joyent.com)",
  "main": "lib/restify.js",
  "dependencies": {
    "formidable": "1.0.2",
    "httpu": "0.0.1",
    "node-uuid": "1.2.0",
    "retry": "0.4.0",
    "semver": "~1.0.8"
  },
  "scripts": {
